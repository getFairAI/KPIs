{
  "name": "fairai-kpi-cache-system",
  "version": "1.0.0",
  "description": "Cache System for KPIs from FairAI",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run tsc-watch",
    "clean": "rimraf ./src/**/bin ./bin ./src/**/tsconfig.tsbuildinfo tsconfig.tsbuildinfo",
    "build": "npm run clean && graphql-codegen && ./node_modules/.bin/tsc -b -v",
    "nodemon": "nodemon --watch src/**/*.ts --watch src/**/*.js --exec node --inspect --loader ts-node/esm ./src/index.ts",
    "tsc-watch": "./node_modules/.bin/tsc-watch -b -v -i -w --noclear --compiler \"./node_modules/.bin/tsc\" --onSuccess \"node --inspect --es-module-specifier-resolution=node ./bin/index.js\"",
    "codegen": "graphql-codegen --config graphql.config.js --project prj3"
  },
  "author": "FairAI - getfair.ai",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.3",
    "bson": "^6.8.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^4.20.0",
    "express-session": "^1.18.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "mongodb": "^6.8.1",
    "mongoose": "^8.6.1",
    "multer": "^1.4.4",
    "node-cron": "^3.0.3",
    "queue-promise": "^2.2.1",
    "run-middleware": "^0.9.10",
    "viem": "^2.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@types/body-parser": "~1.19.5",
    "@types/cors": "~2.8.17",
    "@types/crypto-js": "~4.2.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "~1.18.0",
    "@types/jest": "^29.5.12",
    "@types/multer": "~1.4.12",
    "@types/node": "^22.5.4",
    "@types/node-cron": "~3.0.11",
    "@typescript-eslint/parser": "^8.5.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.6.2"
  },
  "type": "module"
}
